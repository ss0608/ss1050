#include <stdio.h>

int main(void)
{
	int a, b, result;

	printf("Prease Enter Number: ");
	scanf_s("%d", &a);
	printf("Prease Enter Number: ");
	scanf_s("%d", &b);

	result = a + b;
	printf("%d+%d=%d\n", a, b, result);

　　　　result = a - b;
　　　　printf("%d-%d=%d\n", a, b, result);

　　　　result = a * b;
　　　　printf("%d*%d=%d\n", a, b, result);

　　　　result = a /b;
　　　　printf("%d/%d=%d\n", a, b, result);


	return 0;

}
